{% extends 'base.html.twig' %}

{% block body %}
    <h1>Formularioopcion</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ formularioOpcion.id }}</td>
            </tr>
            <tr>
                <th>Opcion</th>
                <td>{{ formularioOpcion.opcion }}</td>
            </tr>
            <tr>
                <th>Fecha</th>
                <td>{% if formularioOpcion.fecha %}{{ formularioOpcion.fecha|date('Y-m-d H:i:s') }}{% endif %}</td>
            </tr>
            <tr>
                <th>Fechatiempo</th>
                <td>{% if formularioOpcion.fechaTiempo %}{{ formularioOpcion.fechaTiempo|date('Y-m-d H:i:s') }}{% endif %}</td>
            </tr>
            <tr>
                <th>Hash</th>
                <td>{{ formularioOpcion.hash }}</td>
            </tr>
            <tr>
                <th>Estado</th>
                <td>{{ formularioOpcion.estado }}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('formularioopcion_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('formularioopcion_edit', { 'id': formularioOpcion.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
